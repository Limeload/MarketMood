<% if @error_message %>
  <p><%= @error_message %></p>
<% else %>
  <h1>End of Day Data for <%= params[:symbol] %></h1>
  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Open</th>
        <th>High</th>
        <th>Low</th>
        <th>Close</th>
        <th>Volume</th>
      </tr>
    </thead>
    <tbody>
      <% @eod_data.each do |entry| %>
        <tr>
          <td><%= entry["date"] %></td>
          <td><%= entry["open"] %></td>
          <td><%= entry["high"] %></td>
          <td><%= entry["low"] %></td>
          <td><%= entry["close"] %></td>
          <td><%= entry["volume"] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
